{
    "manifest_version": 2,
    "name": "Cookie Oven",
    "version": "0.2",
    "permissions": [
        "cookies",
        "tabs",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
         "name": "Click to get URL", 
         "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.5.1.min.js", "content.js"]
        }
      ]
}